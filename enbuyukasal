//en büyük asal sayı bul
#include <iostream>
	using namespace std;

	int enbuyukasalcarpan(int n) {
		int enbuyuk = -1;

		while (n % 2 == 0) {
			enbuyuk = 2;
			n /= 2;
		}
		for (int i = 3; i * i<=n; i += 2) {
			while (n % i == 0) {
				enbuyuk = i;
				n /= i;	
			}
		}
		return enbuyuk;

	}
	int main() {              
		int sayi = 600851475143;
		int enbuyuk = enbuyukasalcarpan(sayi);
		cout<<"En buyuk asal carpan:" << enbuyuk << endl;



	}
