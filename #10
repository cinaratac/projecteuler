#include <iostream>
#include <string>
using namespace std;

bool asalkontrol(int n) {
	for (int i = 2; i*i <= n; i++) {
		if (n % i == 0) return false;
	}
	return true;
}

int main() {
	long long toplam = 0;
	int max = 2000000;
	for (int j = 2; j < max; j++) {
		if (asalkontrol(j)) {
			toplam += j;
		}
	}
	cout << "2 milyonun alt覺ndaki asal say 覺lar覺n toplam覺" << toplam << endl;
}
